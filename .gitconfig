[user]
	name = Ivan Zezyulya
	email = ivan@zezyulya.com
[alias]
	cam = commit -a -m
	st = status
	lol = log --graph --decorate --pretty=oneline --abbrev-commit
	lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
	br = branch
	co = checkout
[apply]
	whitespace = nowarn
[color]
	diff = auto
	status = auto
	branch = auto	 
#[diff] 
#	tool = gvimdiff
[core]
	quotepath = false
	excludesfile = ~/.gitignore_global
[push]
	default = matching
[difftool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/araxiscompare-w.sh -2 \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/araxisgitmerge-w.sh \"$REMOTE\" \"$BASE\" \"$LOCAL\" \"$MERGED\"
	trustExitCode = true
[filter "media"]
	clean = git-media-clean %f
	smudge = git-media-smudge %f
